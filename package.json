{
  "name": "algorithm-learning-platform",
  "version": "1.0.0",
  "type": "module",
  "description": "交互式算法与数据结构学习平台",
  "homepage": "http://blog.zenheart.site/learn-algorithm",
  "keywords": [
    "算法",
    "数据结构",
    "可视化",
    "学习平台"
