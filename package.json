    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.11",
    "terser": "^5.31.0",
    "typescript": "^5.5.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.8.1"
