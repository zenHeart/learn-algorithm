    "deploy": "pnpm run build && gh-pages -d dist",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "verify": "node verify-setup.mjs"
  },
  "dependencies": {
    "@antv/g": "^6.0.0",
    "@antv/g2": "^5.2.0",
    "@antv/util": "^3.3.0",
